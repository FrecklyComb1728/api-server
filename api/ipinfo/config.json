{
  "default_timeout": 1000,
  "retry_count": 3,
  "cache_ttl": 604800,
  "load_balance_strategy": "least_used",
  "ip_headers": [
    {"name": "X-Forwarded-For", "priority": 1},
    {"name": "x-real-ip", "priority": 2},
    {"name": "x-client-ip", "priority": 3},
    {"name": "cf-connecting-ip", "priority": 4},
    {"name": "x-forwarded", "priority": 5},
    {"name": "forwarded-for", "priority": 6},
    {"name": "forwarded", "priority": 7}
  ],
  "response_fields": [
    "ip",
    "prov",
    "city",
    "district",
    "country",
    "isp",
    "lon",
    "lat"
  ],
  "upstream_apis": [
    {
      "name": "bt.cn",
      "url": "https://www.bt.cn/api/panel/get_ip_info?ip={ip}",
      "max_requests": 50,
      "time_window": 60,
      "enabled": true,
      "field_mapping": {
        "ip": "{ip}",
        "prov": "{ip}.province",
        "city": "{ip}.city",
        "country": "{ip}.country",
        "isp": "{ip}.carrier",
        "lon": "{ip}.longitude",
        "lat": "{ip}.latitude"
      }
    },
    {
      "name": "ip9",
      "url": "https://ip9.com.cn/get?ip={ip}",
      "max_requests": 50,
      "time_window": 60,
      "enabled": true,
      "field_mapping": {
        "ip": "data.ip",
        "prov": "data.prov",
        "city": "data.city",
        "country": "data.country",
        "isp": "data.isp",
        "lon": "data.lng",
        "lat": "data.lat"
      }
    },
    {
      "name": "songzixian",
      "url": "https://api.songzixian.com/api/ip?dataSource=GLOBAL_IP&ip={ip}",
      "max_requests": 120,
      "time_window": 60,
      "enabled": true,
      "field_mapping": {
        "ip": "data.ip",
        "prov": "data.province",
        "city": "data.city",
        "district": "data.district",
        "country": "data.country",
        "isp": "data.isp",
        "lon": "data.longitude",
        "lat": "data.latitude"
      }
    },
    {
      "name": "52vmy",
      "url": "https://api.52vmy.cn/api/query/itad/pro?ip={ip}",
      "max_requests": 50,
      "time_window": 60,
      "enabled": true,
      "field_mapping": {
        "ip": "ip",
        "prov": "data.province",
        "city": "data.city",
        "district": "data.district",
        "country": "data.country",
        "isp": "data.isp",
        "lon": "data.longitude",
        "lat": "data.latitude"
      }
    }
  ]
}
